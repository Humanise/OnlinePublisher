					<icon icon="common/folder" text="List" click="dock.setUrl('xml/list.xml')"/>
					<icon icon="common/folder" text="RichText" click="dock.setUrl('xml/richtext.xml')"/>
					<icon icon="common/folder" text="Sites" click="dock.setUrl('xml/sites.xml')"/>
					<icon icon="common/folder" text="Tabs" click="dock.setUrl('xml/tabs.xml')"/>
					<icon icon="common/folder" text="Toolbar" click="dock.setUrl('xml/toolbar.xml')"/>
				</toolbar>
			</tab>
			<tab title="Interfaces">
				<toolbar>
					<icon icon="common/folder" text="Competence" click="dock.setUrl('guis/competence.xml')"/>
